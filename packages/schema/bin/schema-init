#! /usr/bin/env node
const command = require("commander")
const fs = require("fs-extra")
const path = require("path")
const Conf = require("conf")
const config = new Conf({ configName: 'schema' })

command
  .name("schema init")
  .action(function() {
      if (config.size !==0) {
          console.error('not empty')
      } else {
          config.set('version','1')
      }
  })
  .parse(process.argv)

