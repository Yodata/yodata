		"eslint-plugin-promise": "^4.1.1",
		"eslint-plugin-standard": "^4.0.0",
		"jest": "^24.8.0",
		"xo": "^0.24.0"
	},
	"xo": {
		"semicolon": false,
		"plugins": "jest",
		"envs": [
			"commonjs",
			"es6",
			"node",
			"jest/globals"
		],
		"rules": {
			"semi": [
				"error",
				"never"
			],
			"object-curly-spacing": [
				"error",
				"always"
			]
