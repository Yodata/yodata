{
  "name": "@yodata/cli",
  "version": "3.11.20",
  "description": "yodata schema collaboration cli tools",
  "keywords": [
    "schema",
    "structured",
    "data",
    "json",
    "rdfs",
    "schema.org",
    "json-schema"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "bin": {
    "yodata": "./bin/run"
  },
  "files": [
    "/oclif.manifest.json",
    "/bin",
    "/src",
    "/npm-shrinkwrap.json",
    "/yarn-lock.json"
  ],
  "scripts": {
    "test": "npx jest --env test",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@oclif/command": "^1.5.14",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-autocomplete": "^1.1.1",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/plugin-plugins": "^1.10.0",
    "@yodata/cli-tools": "^0.7.17",
    "@yodata/client": "^1.11.17",
    "@yodata/config": "^1.6.15",
    "@yodata/logger": "^2.1.5",
    "assert-plus": "^1.0.0",
    "caller-path": "^2.0.0",
    "chalk": "^3.0.0",
    "fs-extra": "^8.0.0",
    "get-value": "^3.0.1",
    "inquirer": "^8.2.0",
    "js-yaml": "^3.13.1",
    "json-stringify-safe": "^5.0.1",
    "kind-of": "^6.0.3",
    "lodash": "^4.17.11",
    "set-value": "^4.1.0",
    "table": "^6.8.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "@types/get-value": "^3.0.1",
    "@types/set-value": "^2.0.0"
  },
  "oclif": {
    "bin": "yodata",
    "commands": "./src/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-autocomplete"
    ]
  },
  "gitHead": "0e10738f2d67e3e3ba2cd2003162210edfecf83e"
}
