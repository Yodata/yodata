{
	"name": "@yodata/cli",
	"version": "2.0.6",
	"description": "structured data schema developement & collaboration tools",
	"bin": {
		"yodata": "lib/cli.js"
	},
	"main": "lib/index.js",
	"files": [
		"lib"
	],
	"scripts": {
		"test": "xo"
	},
	"keywords": [
		"schema",
		"structured",
		"data",
		"json",
		"rdfs",
		"schema.org",
		"json-schema"
	],
	"author": "yodaveduran <dave@yodata.io> (https://yodata.io/)",
	"license": "MIT",
	"dependencies": {
		"@yodata/config": "@yodata/config",
		"assert-plus": "^1.0.0",
		"clui": "^0.3.6",
		"configstore": "^4.0.0",
		"coverup": "^0.1.1",
		"got": "^9.6.0",
		"inquirer": "^6.2.2",
		"js-yaml": "^3.13.1",
		"lodash": "^4.17.11",
		"mime": "^2.4.2",
		"winston": "^3.2.1",
		"yargs": "^13.2.2"
	},
	"devDependencies": {
		"@types/assert-plus": "^1.0.4",
		"@types/configstore": "^4.0.0",
		"@types/got": "^9.4.0",
		"@types/inquirer": "0.0.43",
		"@types/js-yaml": "^3.12.1",
		"@types/lodash": "^4.14.123",
		"@types/mime": "^2.0.1",
		"@types/winston": "^2.4.4",
		"@types/yargs": "^13.0.0"
	},
	"xo": {
		"semicolon": false,
		"plugins": "jest",
		"envs": [
			"node",
			"jest/globals"
		],
		"rules": {
			"semi": [
				"error",
				"never"
			]
		}
	}
}
