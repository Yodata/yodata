{
	"name": "@yodata/cli",
	"version": "3.0.2-alpha.5",
	"description": "yodata cli",
	"author": "yodaveduran <dave@yodata.io> (https://yodata.io/)",
	"bin": {
		"yodata": "./bin/run"
	},
	"dependencies": {
		"@oclif/command": "^1",
		"@oclif/config": "^1",
		"@oclif/plugin-help": "^2",
		"@yodata/client": "file:../client",
		"@yodata/config": "file:../config",
		"@yodata/logger": "file:../logger",
		"@yodata/transform": "file:../transform",
		"assert-plus": "^1.0.0",
		"caller-path": "^2.0.0",
		"chalk": "^2.4.2",
		"cli-ux": "^5.2.1",
		"fs-extra": "^8.0.0",
		"got": "^9.6.0",
		"inquirer": "^6.2.2",
		"js-yaml": "^3.13.1",
		"json-stringify-safe": "^5.0.1",
		"kind-of": "^6.0.2",
		"lodash": "^4.17.11",
		"table": "^5.3.3",
		"yargs": "^13.2.2",
		"yodata-plugin-inbox": "file:../yodata-plugin-inbox"
	},
	"files": [
		"/bin",
		"/src",
		"/npm-shrinkwrap.json",
		"/oclif.manafest.json",
		"!__tests__"
	],
	"gitHead": "a7c7e14c460b83cc7b084ae89b050d8f27f2c427",
	"keywords": [
		"schema",
		"structured",
		"data",
		"json",
		"rdfs",
		"schema.org",
		"json-schema"
	],
	"license": "MIT",
	"main": "lib/index.js",
	"oclif": {
		"bin": "yodata",
		"commands": "./src/commands",
		"plugins": [
			"@oclif/plugin-help"
		]
	},
	"scripts": {
		"test": "npx jest",
		"postpack": "rm -f oclif.manifest.json",
		"posttest": "eslint .",
		"prepack": "oclif-dev manifest && oclif-dev readme",
		"version": "oclif-dev readme && git add README.md"
	},
	"xo": {
		"envs": [
			"commonjs",
			"es6",
			"node",
			"jest/globals"
		],
		"plugins": "jest",
		"rules": {
			"object-curly-spacing": [
				"error",
				"always"
			],
			"semi": [
				"error",
				"never"
			]
		},
		"semicolon": false
	},
	"devDependencies": {
		"@oclif/test": "^1.2.4",
		"@oclif/dev-cli": "^1",
    "globby": "^8"
	}
}
