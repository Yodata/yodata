{
  "name": "@yodata/cli",
  "version": "3.9.0",
  "description": "yodata schema collaboration cli tools",
  "keywords": [
    "schema",
    "structured",
    "data",
    "json",
    "rdfs",
    "schema.org",
    "json-schema"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "bin": {
    "yodata": "./bin/run"
  },
  "files": [
    "/oclif.manifest.json",
    "/bin",
    "/src",
    "/npm-shrinkwrap.json",
    "/yarn-lock.json"
  ],
  "scripts": {
    "test": "npx jest",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@oclif/command": "^1.5.14",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-autocomplete": "^0.1.4",
    "@oclif/plugin-help": "^2.2.0",
    "@oclif/plugin-plugins": "^1.7.8",
    "@yodata/cli-tools": "^0.5.0",
    "@yodata/client": "^1.9.0",
    "@yodata/config": "^1.6.11",
    "@yodata/logger": "^2.0.8",
    "assert-plus": "^1.0.0",
    "caller-path": "^2.0.0",
    "chalk": "^3.0.0",
    "fs-extra": "^8.0.0",
    "get-value": "^3.0.1",
    "inquirer": "^6.3.1",
    "js-yaml": "^3.13.1",
    "json-stringify-safe": "^5.0.1",
    "kind-of": "^6.0.3",
    "lodash": "^4.17.11",
    "set-value": "^3.0.1",
    "table": "^5.3.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "@types/get-value": "^3.0.1",
    "@types/set-value": "^2.0.0"
  },
  "oclif": {
    "bin": "yodata",
    "commands": "./src/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-autocomplete"
    ]
  },
  "gitHead": "a7c7e14c460b83cc7b084ae89b050d8f27f2c427"
}
