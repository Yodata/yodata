{
	"name": "@yodata/cli",
	"version": "1.0.0",
	"private": true,
	"description": "structured data schema developement & collaboration tools",
	"bin": {
		"yodata": "./bin/yodata"
	},
	"scripts": {
		"test": "xo"
	},
	"keywords": [
		"schema",
		"structured",
		"data",
		"json",
		"rdfs",
		"schema.org",
		"json-schema"
	],
	"author": "yodaveduran <dave@yodata.io> (https://yodata.io/)",
	"license": "MIT",
	"dependencies": {
		"aws-sdk": "^2.395.0",
		"clui": "^0.3.6",
		"configstore": "^4.0.0",
		"got": "^9.6.0",
		"inquirer": "^6.2.2",
		"winston": "^3.2.1"
	},
	"devDependencies": {
		"@types/aws-sdk": "^2.7.0",
		"@types/configstore": "^4.0.0",
		"@types/got": "^9.4.0",
		"@types/inquirer": "0.0.43",
		"@types/winston": "^2.4.4"
	},
	"xo": {
		"semicolon": false,
		"plugins": "jest",
		"envs": [
			"node",
			"jest/globals"
		],
		"rules": {
			"semi": [
				"error",
				"never"
			]
		}
	}
}
