{
	"name": "@yodata/cli",
	"version": "3.0.2-alpha.5",
	"description": "yodata cli",
	"author": "yodaveduran <dave@yodata.io> (https://yodata.io/)",
	"bin": {
		"yodata": "./bin/run"
	},
	"dependencies": {
		"@oclif/command": "^1",
		"@oclif/config": "^1",
		"@oclif/plugin-help": "^2",
		"@yodata/client": "^1.0.2-alpha.5",
		"@yodata/logger": "^1.0.1-alpha.0",
		"@yodata/transform": "^1.0.1-alpha.0",
		"assert-plus": "^1.0.0",
		"caller-path": "^2.0.0",
		"chalk": "^2.4.2",
		"fs-extra": "^8.0.0",
		"got": "^9.6.0",
		"inquirer": "^6.2.2",
		"js-yaml": "^3.13.1",
		"json-stringify-safe": "^5.0.1",
		"kind-of": "^6.0.2",
		"lodash": "^4.17.11",
		"table": "^5.3.3",
		"yargs": "^13.2.2",
		"yodata-plugin-inbox": "^0.0.2-alpha.1"
	},
	"devDependencies": {
		"@oclif/dev-cli": "^1",
		"@oclif/test": "^1",
		"@types/assert-plus": "^1.0.4",
		"@types/chalk": "^2.2.0",
		"@types/fs-extra": "^5.1.0",
		"@types/got": "^9.4.0",
		"@types/inquirer": "0.0.43",
		"@types/jest": "^24.0.12",
		"@types/js-yaml": "^3.12.1",
		"@types/json-stringify-safe": "^5.0.0",
		"@types/kind-of": "^6.0.0",
		"@types/lodash": "^4.14.123",
		"@types/table": "^4.0.5",
		"@types/yargs": "^13.0.0",
		"eslint": "^5.16.0",
		"eslint-config-oclif": "^3.1",
		"eslint-config-standard": "^12.0.0",
		"eslint-plugin-import": "^2.17.2",
		"eslint-plugin-node": "^9.0.1",
		"eslint-plugin-promise": "^4.1.1",
		"eslint-plugin-standard": "^4.0.0",
		"jest": "^24.8.0",
		"xo": "^0.24.0"
	},
	"files": [
		"/bin",
		"/src",
		"/npm-shrinkwrap.json",
		"/oclif.manafest.json",
		"!__tests__"
	],
	"gitHead": "a7c7e14c460b83cc7b084ae89b050d8f27f2c427",
	"keywords": [
		"schema",
		"structured",
		"data",
		"json",
		"rdfs",
		"schema.org",
		"json-schema"
	],
	"license": "MIT",
	"main": "lib/index.js",
	"oclif": {
		"bin": "yodata",
		"commands": "./src/commands",
		"plugins": [
			"@oclif/plugin-help"
		]
	},
	"scripts": {
		"test": "npx jest",
		"postpack": "rm -f oclif.manifest.json",
		"posttest": "eslint .",
		"prepack": "oclif-dev manifest && oclif-dev readme",
		"version": "oclif-dev readme && git add README.md"
	},
	"xo": {
		"envs": [
			"commonjs",
			"es6",
			"node",
			"jest/globals"
		],
		"plugins": "jest",
		"rules": {
			"object-curly-spacing": [
				"error",
				"always"
			],
			"semi": [
				"error",
				"never"
			]
		},
		"semicolon": false
	}
}
