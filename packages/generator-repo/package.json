{
  "private": true,
  "name": "@yodata/generator-yodata-repo",
  "version": "0.0.0",
  "description": "Yeoman generator for yodata oss projects",
  "author": "Dave Duran <dave@yodata.io>",
  "keywords": [
    "yeoman",
