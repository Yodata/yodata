{
	"name": "@yodata/transform",
	"version": "0.3.2",
	"description": "declarative, JSON transformation library",
	"main": "src/index.js",
	"access": "public",
	"homepage": "https://github.com/Yodata/yodata/tree/master/packages/transform#readme",
	"engines": {
		"node": ">=8.10"
	},
	"files": [
		"src"
	],
	"scripts": {
		"commit": "npx git-cz",
		"test": "xo && jest --env node"
	},
	"author": "yodaveduran <dave@yodata.io> (https://yodata.io/)",
	"license": "MIT",
	"dependencies": {
		"caller-path": "^2.0.0",
		"debug": "^4.1.1",
		"fs-extra": "^7.0.1",
		"immutable": "^4.0.0-rc.12",
		"js-yaml": "^3.12.1",
		"jsonata": "^1.6.4",
		"kind-of": "^6.0.2",
		"lodash": "^4.17.11",
		"sort-object-keys": "^1.1.2"
	},
	"directories": {
		"example": "examples",
		"lib": "src",
		"test": "__tests__"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.3.1",
		"@types/jsonata": "^1.3.1",
		"eslint-plugin-jest": "^22.1.3",
		"jest": "^24.0.0",
		"regenerator-runtime": "^0.13.1",
		"xo": "^0.24.0"
	},
	"keywords": [
		"json",
		"schema",
		"transform",
		"jsonschema",
		"json-ld"
	],
	"xo": {
		"semicolon": false,
		"plugin": "jest",
		"envs": [
			"node",
			"jest/globals"
		],
		"rules": {
			"semi": [
				"error",
				"never"
			]
		}
	},
	"jest": {
		"testEnvironment": "node"
	}
}
