{
	"name": "@yodata/transform",
	"version": "0.2.6",
	"description": "declarative, JSON transformation library",
	"main": "lib/index.js",
	"access": "public",
	"engines": {
		"node": ">=8.10"
	},
	"scripts": {
		"clean": "rm -rf lib/*",
		"commit": "npx git-cz",
		"build": "npm run clean & babel src/ -d lib/",
		"prepare": "npm run build",
		"test": "jest --env node"
	},
	"author": "yodaveduran <dave@yodata.io> (https://yodata.io/)",
	"license": "MIT",
	"dependencies": {
		"ajv": "^6.5.5",
		"ajv-keywords": "^3.2.0",
		"fs-extra": "^7.0.1",
		"immutable": "^4.0.0-rc.12",
		"js-yaml": "^3.12.0",
		"kind-of": "^6.0.2",
		"lodash": "^4.17.11",
		"ow": "^0.8.0",
		"sort-object-keys": "^1.1.2"
	},
	"directories": {
		"example": "examples",
		"lib": "lib",
		"test": "__tests__"
	},
	"devDependencies": {
		"@babel/cli": "^7.1.2",
		"@babel/core": "^7.2.2",
		"@babel/plugin-proposal-class-properties": "^7.1.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.2.0",
		"@babel/preset-flow": "^7.0.0",
		"babel-core": "^7.0.0-bridge.0",
		"babel-jest": "^23.6.0",
		"debug": "^4.1.1",
		"eslint-config-xo-flow": "^0.1.0",
		"flow-bin": "^0.85.0",
		"jest": "^23.6.0",
		"regenerator-runtime": "^0.13.1",
		"xo": "^0.23.0"
	},
	"xo": {
		"semicolon": false
	},
	"eslintConfig": {
		"extends": [
			"xo",
			"xo-flow"
		]
	},
	"keywords": [
		"json",
		"schema",
		"transform",
		"jsonschema",
		"json-ld"
	]
}
