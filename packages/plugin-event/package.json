{
  "name": "@yodata/plugin-event",
  "version": "0.1.0",
  "author": "yodaveduran <dave@yodata.io> (https://yodata.io/)",
  "description": "manage events and subscriptions in @yodata/cli",
  "bugs": "https://github.com/yodata/yodata/issues",
  "dependencies": {
    "@oclif/command": "^1.5.14",
    "@oclif/config": "^1",
    "@yodata/cli-tools": "^0.2.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@types/eslint": "^4.16.6",
    "@types/globby": "^9.1.0",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "globby": "^8"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "files": [
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/yodata/yodata",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./src/commands",
    "bin": "yodata event",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yodata/yodata.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "npx jest",
    "version": "oclif-dev readme && git add README.md"
  }
}
